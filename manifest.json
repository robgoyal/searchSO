{
    "manifest_version": 2,
    "name": "Search Stackoverflow",
    "description": "This extension returns search results from Stackoverflow within popup",
    "version": "1.0",

    "browser_action": {
        "default_title": "searchSO",
        "default_popup": "popup.html"
    },

    "background": {
        "scripts": ["popup.js"],
        "persistent": false
    },

    "permissions": ["https://api.stackexchange.com/2.2/"]
}